{% extends '::bootstrap.html.twig' %}
{% block titulo %}
    Editar Voice Porta
{% endblock %}
{% block body -%}
    

     {{ form_start(edit_form) }}
    <div class="form-group">
        {{ form_label(edit_form.numero) }}
        {{ form_widget(edit_form.numero, { 'attr': { 'class': 'form-control' } }) }}
    </div>
    <div class="form-group">
        {{ form_label(edit_form.central) }}
        {{ form_widget(edit_form.central, { 'attr': { 'class': 'form-control' } }) }}
    </div>    
    <div class="form-group">
        {{ form_label(edit_form.distribuicao) }}
        {{ form_widget(edit_form.distribuicao, { 'attr': { 'class': 'form-control' } }) }}
    </div>
    <div class="form-group">
        {{ form_label(edit_form.ramal) }}
        {{ form_widget(edit_form.ramal, { 'attr': { 'class': 'form-control' } }) }}
    </div>
    <div class="form-group">
        {{ form_label(edit_form.voicePanel) }}
        {{ form_widget(edit_form.voicePanel, { 'attr': { 'class': 'form-control' } }) }}
    </div>
    <div class="form-group">
        {{ form_label(edit_form.pporta) }}
        {{ form_widget(edit_form.pporta, { 'attr': { 'class': 'form-control' } }) }}
    </div>
    
     {{ form_widget(edit_form.submit, { 'attr': {'class': 'btn btn-sm btn-success' } }) }} <a href="{{ path('voiceporta') }}" class="btn btn-sm btn-info">Voltar para a Lista</a>
    
    {{ form_end(edit_form) }}
    
   {{ form_start(delete_form) }}
        {{ form_widget(delete_form.submit, {'attr': {'class': 'btn btn-sm btn-danger' } }) }} 
    {{ form_end(delete_form) }}  
{% endblock %}
