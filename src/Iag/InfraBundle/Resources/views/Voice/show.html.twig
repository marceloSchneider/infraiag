{% extends '::bootstrap.html.twig' %}

{% block body -%}
    <h1>Voice</h1>

    <table class="table table-striped table-bordered">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Par</th>
                <td>{{ entity.par }}</td>
            </tr>
            <tr>
                <th>Ramal</th>
                <td>{{ entity.ramal }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('voice') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('voice_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
