{% extends '::bootstrap.html.twig' %}

{% block body -%}
    <h1>Vlan</h1>

    <table class="table table-striped table-bordered">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Descricao</th>
                <td>{{ entity.descricao }}</td>
            </tr>
            <tr>
                <th>Endereco</th>
                <td>{{ entity.endereco }}</td>
            </tr>
            <tr>
                <th>Vlanrange</th>
                <td>{{ entity.vlanRange }}</td>
            </tr>
        </tbody>
    </table>
    {{ form_start(delete_form) }}
        <a href="{{ path('vlan_edit', { 'id': entity.id }) }}"class="btn btn-sm btn-primary">Editar</a> <a href="{{ path('vlan') }}" class="btn btn-sm btn-info">Voltar para a lista</a> {{ form_widget(delete_form.submit, {'attr': { 'class': 'btn btn-sm btn-danger' } } ) }}
    {{ form_end(delete_form) }}
{% endblock %}
