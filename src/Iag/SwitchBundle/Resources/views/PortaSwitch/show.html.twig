{% extends '::bootstrap.html.twig' %}
{% block titulo %}
    Porta do Switch
{% endblock %}
{% block body -%}

    <table class="table table-striped table-bordered">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Numporta</th>
                <td>{{ entity.numPorta }}</td>
            </tr>
            <tr>
                <th>Ispoe</th>
                <td>{{ entity.isPOE }}</td>
            </tr>
            <tr>
                <th>Poestatus</th>
                <td>{{ entity.pOEStatus }}</td>
            </tr>
        </tbody>
    </table>
    {{ form_start(delete_form) }}
        <a href="{{ path('portaswitch_edit', { 'id': entity.id }) }}"class="btn btn-sm btn-primary">Editar</a> <a href="{{ path('portaswitch') }}" class="btn btn-sm btn-info">Voltar para a lista</a> {{ form_widget(delete_form.submit, {'attr': { 'class': 'btn btn-sm btn-danger' } } ) }}
    {{ form_end(delete_form) }}
{% endblock %}
