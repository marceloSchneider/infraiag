{% extends '::bootstrap.html.twig' %}
{% block titulo %}
    Detalhes do switch {{ entity.switchcommon.hostname }}
{% endblock %}
{% block body -%}
    

    <table class="table table-striped table-bordered">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Hostname</th>
                <td>{{ entity.switchcommon.hostname }}</td>
            </tr>
            <tr>
                <th>Ip</th>
                <td>{{ entity.switchcommon.ip }}</td>
            </tr>
            <tr>
                <th>Numportas</th>
                <td>{{ entity.numPortas }}</td>
            </tr>
            <tr>
                <th>Marca</th>
                <td>{{ entity.marca }}</td>
            </tr>
            <tr>
                <th>Patrimonio</th>
                <td>{{ entity.patrimonio }}</td>
            </tr>
            <tr>
                <th>Pilhafisica</th>
                <td>{{ entity.posicaoPilha }}</td>
            </tr>
        </tbody>
    </table>
    {{ form_start(delete_form) }}
        <a href="{{ path('switchs_edit', { 'id': entity.id }) }}"class="btn btn-sm btn-primary">Editar</a> <a href="{{ path('switchs') }}" class="btn btn-sm btn-info">Voltar para a lista</a> {{ form_widget(delete_form.submit, {'attr': { 'class': 'btn btn-sm btn-danger' } } ) }}
    {{ form_end(delete_form) }}
{% endblock %}
