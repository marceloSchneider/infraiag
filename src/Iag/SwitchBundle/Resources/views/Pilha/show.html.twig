{% extends '::base.html.twig' %}
{% block titulo %}
    Pilha
{% endblock %}
{% block body -%}
    

    <table class="table table-striped table-bordered">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Ip</th>
                <td>{{ entity.ip }}</td>
            </tr>
            <tr>
                <th>Hostname</th>
                <td>{{ entity.hostname }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('pilha') }}">
            Voltar para a lista
        </a>
    </li>
    <li>
        <a href="{{ path('pilha_edit', { 'id': entity.id }) }}">
            Editar
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
